msgid "404 Error Page"
msgstr "Página de error 404"

#: inc/class-404page-admin.php:153
msgid "&mdash; NONE (WP default 404 page) &mdash;"
msgstr "&mdash; NINGUNA (página 404 por defecto de WP) &mdash;"

